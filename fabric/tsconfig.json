{
  "target": "es5",
  "lib": ["es6", "dom"],
  "sourceMap": true,
  "allowJs": true,
  "jsx": "react",
  "moduleResolution": "node",
  "rootDir": "src",
  "forceConsistentCasingInFileNames": true,
  "noImplicitReturns": true,
  "noImplicitThis": true,
  "noImplicitAny": true,
  "strictNullChecks": true,
  "suppressImplicitAnyIndexErrors": true,
  "noUnusedLocals": true,
  "extends": "node_modules/assemblyscript/std/assembly.json",
  "include": [
    "./index.ts"
  ]
}

