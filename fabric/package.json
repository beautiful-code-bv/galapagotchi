{
  "name": "fabric",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "asbuild:untouched": "./node_modules/.bin/asc index.ts -b build/untouched.wasm -t build/untouched.wat --use Math=JSMath --sourceMap --validate",
    "asbuild:optimized": "asc index.ts -b ../client/build/fabric.wasm -t build/optimized.wat --sourceMap --validate --optimize",
    "build": "npm run asbuild:untouched && npm run asbuild:optimized",
    "sym": "wasm-as build/optimized.wat -s symbols.txt -o build/sym.wasm"
  },
  "author": "Gerald A. de Jong",
  "license": "",
  "devDependencies": {
    "assemblyscript": "github:AssemblyScript/assemblyscript",
    "typescript": "^2.9.2"
  }
}
