  "main": "index.ts",
  "license": "MIT",
  "engineStrict": true,
  "engines": {
    "node": "11.10.1"
  },
  "scripts": {
    "start": "node $NODE_DEBUG_OPTION node_modules/.bin/ts-node index.ts",
    "start:daemon": "pm2 start process.yml",
    "check": "tsc -p . --noemit",
    "test": "mocha -r ts-node/register test/*.test.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "lint": "tslint --fix -p tsconfig.json",
    "preinstall": "node -e 'if(!/yarn\\.js$/.test(process.env.npm_execpath))throw new Error(\"Use yarn instead\")'"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/express-validator": "^3.0.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.10.4",
    "@types/uuid": "^3.4.4",
    "axios": "^0.18.0",
    "bignumber.js": "^8.0.2",
    "commander": "^2.19.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-validator": "latest",
    "http-status-codes": "^1.3.1",
    "morgan": "^1.9.1",
    "pg": "^7.9.0",
    "pm2": "^3.4.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typeorm": "0.2.16",
    "typescript": "^3.3.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "chai": "^4.2.0",
    "mocha": "^6.0.0"
  }
}
